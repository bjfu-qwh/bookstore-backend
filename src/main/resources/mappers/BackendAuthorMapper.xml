<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.edu.bookstore.backend.author.mapper.BackendAuthorMapper">
    <insert id="addAuthor" parameterType="org.edu.bookstore.backend.author.entity.AuthorInfo">
        insert into t_author_info(name, nation, url, brief)
        values (#{author.name}, #{author.nation}, #{author.url}, #{author.brief});
    </insert>
</mapper>